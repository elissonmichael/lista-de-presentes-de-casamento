<h3>Administrar Lista</h3>
<%= render "materiais/link_inserir", model: 'imagem' %>
<table class="table table-striped table-responsive">
  <thead>
    <tr>
      <th>Imagem</th>
      <th>Nome</th>
      <th>Situação</th>
      <th>Modificado Em</th>
      <th>Opções</th>
    </tr>
  </thead>
  <tbody>
    <% @imagens.each do |imagem| %>
      <tr>
        <td><%= image_tag(imagem.url, class:"img-thumbnail rounded border mx-auto") %></td>
        <td><%= imagem.presente %></td>
        <td><%= imagem.disponibilidade %></td>
        <td><%= imagem.presenteado_em %></td>
        <td>
          <div class="btn-group" role="group">
            <%= render "materiais/links_editar_excluir", model: imagem, classe: "btn-secondary" %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= render "materiais/link_inserir", model: 'imagem' %>
